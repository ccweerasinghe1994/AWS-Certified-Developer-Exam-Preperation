{
  "compilerOptions": {
    "target": "ES5",
    "strict": true,
    "preserveConstEnums": true,
    "noEmit": false,
    "outDir": "../.aws-sam/build/HelloWorldFunction",
    "rootDir": "./",
    "sourceMap": true,
    "module": "es2015",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
  },
  "exclude": [
    "node_modules",
    "**/*.test.ts",
    "jest.config.ts"
  ]
}